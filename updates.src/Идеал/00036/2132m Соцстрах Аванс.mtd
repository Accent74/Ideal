Sub Method_OnCalc(Tr)
	Dim s

	With Tr
		.Tariff = .DependsW.Sum
		s = Round2(.Tariff * .Sum2 / 100, 2)
		.Sum1 = s
		.Sum = s
	End With

End Sub

Sub Method_OnApply(Tr)
	With Tr
		.Sum2 = 1	' 1% 		Соцстрах 
	End With
End Sub
